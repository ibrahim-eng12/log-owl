{
    "name": "ibrahim-eng12/laravel-log-viewer",
    "description": "A Laravel package to view, navigate, filter logs with descriptions and solutions",
    "keywords": ["laravel", "logs", "viewer", "debug", "error-handling"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ibrahim Alharasis",
            "email": "ibrahim@himacode.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Ibrah\\LaravelLogViewer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ibrah\\LaravelLogViewer\\LogViewerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
